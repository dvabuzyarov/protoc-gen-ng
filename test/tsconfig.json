{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "rootDir": ".",
    "noImplicitAny": false,
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": [
      "node"
    ],
    "esModuleInterop": true
  },
  "compileOnSave": false
}
